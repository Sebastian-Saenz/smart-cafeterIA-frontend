{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "functions": {
        "app.py": {
            "runtime": "python3.9",
            "handler": "create_app"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}